{
    "output": {
        "filePath": "repomix-output.md",
        "style": "markdown",
        "removeComments": false,
        "removeEmptyLines": false,
        "topFilesLength": 20,
        "showLineNumbers": false,
        "copyToClipboard": false
    },
    "include": [
        "README.md",
        "Directory.Build.props",
        "Directory.Packages.props",
        "src/Directory.Build.props",
        "test/Directory.Build.props",

        "src/HttpServer/HttpServer.csproj",
        "src/HttpServer/GlobalUsings.cs",
        "src/HttpServer/Program.cs",
        "src/HttpServer/SystemDateTime.cs",
        "src/HttpServer/Guard.cs",
        "src/HttpServer/Tools/DateTool.cs",
        "src/HttpServer/Tools/EchoTool.cs",
        "src/HttpServer/Tools/PingPongTool.cs",

        "src/StdioServer/StdioServer.csproj",
        "src/StdioServer/GlobalUsings.cs",
        "src/StdioServer/Program.cs",
        "src/StdioServer/SystemDateTime.cs",
        "src/StdioServer/Guard.cs",
        "src/StdioServer/Tools/DateTool.cs",
        "src/StdioServer/Tools/EchoTool.cs",
        "src/StdioServer/Tools/PingPongTool.cs",

        "src/Client/Client.csproj",
        "src/Client/GlobalUsings.cs",
        "src/Client/appsettings.json",
        "src/Client/Program.cs",
        "src/Client/App.cs",
        "src/Client/App.Console.cs",
        "src/Client/App.Commands.cs",
        "src/Client/Extensions/Extensions.CallToolResponse.cs",
        "src/Client/Extensions/Extensions.IMcpClient.cs",
        "src/Client/Extensions/Extensions.McpClientTool.cs",
        "src/Client/Tool/McpToolProperty.cs",
        "src/Client/Tool/McpTools.cs",
        "src/Client/Tool/McpToolSchema.cs",
        "src/Client/Properties/launchSettings.json",

        "test/Unit/Unit.csproj",
        "test/Unit/GlobalUsings.cs",
        "test/Unit/Setup.cs",
        "test/Unit/test.runsettings",
        "test/Unit/StdioServer/Tools/DateTool.Test.cs",
        "test/Unit/StdioServer/Tools/EchoTool.Test.cs",
        "test/Unit/StdioServer/Tools/PingPongTool.Test.cs"
    ],
    "ignore": {
        "useGitignore": false,
        "useDefaultPatterns": false,
        "customPatterns": [
            "**/bin/**",
            "**/obj/**"
        ]
    },
    "security": {
        "enableSecurityCheck": true
    }
}
