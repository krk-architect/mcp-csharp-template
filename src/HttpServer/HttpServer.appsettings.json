{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http.HttpClient": "Warning"
        },
        "Debug": {
            "LogLevel": {
                "Default": "Information",
                "Microsoft.AspNetCore": "Warning",
                "System.Net.Http.HttpClient": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Information",
                "Microsoft.AspNetCore": "Warning",
                "System.Net.Http.HttpClient": "Warning"
            },
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "[yyyy-MM-dd HH::mm:ss] ",
                "ColorBehavior": "Enabled"
            }
        }
    }
}